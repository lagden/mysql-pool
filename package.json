{
  "name": "@tadashi/mysql-pool",
  "version": "1.0.2",
  "description": "MySQL Pooling connections",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "DEBUG=mysql-pool:* NODE_ENV=test ava",
    "pretest-cov": "npm run pretest",
    "test-cov": "NODE_ENV=test nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/mysql-pool.git"
  },
  "bugs": {
    "url": "https://github.com/lagden/mysql-pool/issues"
  },
  "keywords": [
    "mysql",
    "pool",
    "connection",
    "query"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "@tadashi/debug": "2.1.0",
    "mysql": "2.15.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "nyc": "12.0.1",
    "xo": "0.21.1"
  }
}
