{
  "name": "@tadashi/mysql-pool",
  "version": "1.3.0",
  "description": "MySQL Pooling connections",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lagden/mysql-pool.git"
  },
  "bugs": {
    "url": "https://github.com/lagden/mysql-pool/issues"
  },
  "keywords": [
    "mysql",
    "pool",
    "connection",
    "query"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  },
  "dependencies": {
    "debug": "4.1.1",
    "mysql": "2.17.1"
  },
  "devDependencies": {
    "ava": "2.1.0",
    "xo": "0.24.0"
  }
}
